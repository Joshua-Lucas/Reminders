{
  "name": "reminder_backend",
  "version": "1.0.0",
  "description": "A reminder app created for my wife and I",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "migrate:create": "db-migrate create --migrations-dir=./Database/migrations --config=./Database/database.json",
    "migrate:up": "db-migrate up --migrations-dir=./Database/migrations --config=./Database/database.json",
    "migrate:down": "db-migrate down --migrations-dir=./Database/migrations --config=./Database/database.json",
    "migrate:reset": "db-migrate reset --migrations-dir=./Database/migrations --config=./Database/database.json",
    "migrate:refresh": "npm run migrate:reset && npm run migrate:up"
  },
  "author": "JoshuaLucas",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.6.3",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.20",
    "@types/pg": "^7.14.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "pg": "^8.2.2"
  }
}
